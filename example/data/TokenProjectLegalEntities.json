{
  "schema": {
    "type": "object",
    "title": "Token Project Legal Entities",
    "properties": {
      "codeRepositoryURL": {
        "title": "Code Repository",
        "type": "string"
      },
      "isPermissionedNetwork": {
        "title": "Platform Is Permissioned",
        "type": "boolean"
      },
      "legalEntitiesList": {
        "title": "Project Legal Entities",
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "hasSecuritiesLicense": {
              "title": "Has Securities License",
              "type": "boolean"
            },
            "legalName": {
              "title": "Legal Name",
              "type": "string"
            },
            "LEICode": {
              "title": "LEI Code",
              "type": "string"
            },
            "registrationELFCode": {
              "title": "ELF Code",
              "type": "string"
            },
            "registrationJurisdiction": {
              "title": "Jurisdiction",
              "type": "string"
            },
            "registrationRACode": {
              "title": "Registration Authority Code",
              "type": "string"
            },
            "role": {
              "title": "Role",
              "type": "string"
            }
          }
        }
      },
      "name": {
        "title": "Project Name",
        "type": "string"
      },
      "roadmapURL": {
        "title": "Project Roadmap URL",
        "type": "string"
      },
      "underlyingPlatformName": {
        "title": "Project Platform",
        "type": "string"
      },
      "websiteURL": {
        "title": "Project Website URL",
        "type": "string"
      },
      "whitepaperURL": {
        "title": "Project Whitepaper URL",
        "type": "string"
      }
    }
  },
  "form": [
    {
      "type": "fieldset",
      "items": [
        {
          "key": "legalEntitiesList",
          "add": "New Legal Entity",
          "style": {
            "add": "legalEntitiesList"
          },
          "items": [
            "legalEntitiesList[].legalName",
            "legalEntitiesList[].role",
            "legalEntitiesList[].LEICode",
            "legalEntitiesList[].hasSecuritiesLicense",
            "legalEntitiesList[].registrationELFCode",
            "legalEntitiesList[].registrationRACode",
            "legalEntitiesList[].registrationJurisdiction"
          ]
        }
      ]
    }
  ]
}
