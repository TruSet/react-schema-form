{
  "path": "/currentCoupon",
  "schema": {
    "type": "object",
    "title": "Current Coupon",
    "properties": {
      "accrualDate": {
        "title": "Accrual Date",
        "type": "date"
      },
      "dayCountBasis": {
        "title": "Accrual Basis",
        "type": "string"
      },
      "paymentDate": {
        "title": "Payment Date",
        "type": "date"
      },
      "rate": {
        "title": "Rate",
        "type": "number"
      },
      "type": {
        "title": "Rate Type",
        "type": "string",
        "enum": [
          null,
          "Fixed",
          "Floating"
        ]
      }
    }
  },
  "form": [
    {
      "type": "fieldset",
      "items": [
        "type",
        "rate",
        "paymentDate",
        "accrualDate",
        "dayCountBasis"
      ]
    }
  ]
}
