{
  "schema": {
    "type": "object",
    "title": "Token Project Details",
    "properties": {
      "advisorsList": {
        "title": "Project Advisors",
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "name": {
              "title": "Advisor Name",
              "type": "string"
            }
          }
        }
      },
      "codeRepositoryURL": {
        "title": "Code Repository",
        "type": "string"
      },
      "isPermissionedNetwork": {
        "title": "Platform Is Permissioned",
        "type": "boolean"
      },
      "name": {
        "title": "Project Name",
        "type": "string"
      },
      "roadmapURL": {
        "title": "Project Roadmap URL",
        "type": "string"
      },
      "socialMediaAccountsList": {
        "title": "Project Social Media Acounts",
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "otherAccountType": {
              "title": "Other Account Type",
              "type": "string"
            },
            "type": {
              "title": "Account Type",
              "type": "string",
              "enum": [
                null,
                "Bitcointalk",
                "Blog",
                "Facebook",
                "Instagram",
                "LinkedIn",
                "Medium",
                "Other",
                "Reddit",
                "Slack",
                "Steemit",
                "Telegram",
                "Twitter",
                "WeChat",
                "YouTube"
              ]
            },
            "URL": {
              "title": "Account URL",
              "type": "string"
            }
          }
        }
      },
      "teamMembersList": {
        "title": "Project Team Members",
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "name": {
              "title": "Member Name",
              "type": "string"
            },
            "role": {
              "title": "Member Role",
              "type": "string"
            },
            "socialMediaAccountsList": {
              "title": "Social Media Accounts",
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "otherAccountType": {
                    "title": "Other Account Type",
                    "type": "string"
                  },
                  "type": {
                    "title": "Account Type",
                    "type": "string",
                    "enum": [
                      null,
                      "Bitcointalk",
                      "Blog",
                      "Facebook",
                      "Instagram",
                      "LinkedIn",
                      "Medium",
                      "Other",
                      "Reddit",
                      "Slack",
                      "Steemit",
                      "Telegram",
                      "Twitter",
                      "WeChat",
                      "YouTube"
                    ]
                  },
                  "URL": {
                    "title": "Account URL",
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      },
      "underlyingPlatformName": {
        "title": "Project Platform",
        "type": "string"
      },
      "websiteURL": {
        "title": "Project Website URL",
        "type": "string"
      },
      "whitepaperURL": {
        "title": "Project Whitepaper URL",
        "type": "string"
      }
    }
  },
  "form": [
    {
      "type": "fieldset",
      "items": [
        "name",
        "websiteURL",
        "whitepaperURL",
        "roadmapURL",
        "codeRepositoryURL",
        "underlyingPlatformName",
        "isPermissionedNetwork"
      ]
    },
    {
      "type": "fieldset",
      "items": [
        {
          "key": "socialMediaAccountsList",
          "add": "New Account",
          "style": {
            "add": "socialMediaAccountsList"
          },
          "items": [
            "socialMediaAccountsList[].type",
            "socialMediaAccountsList[].otherAccountType",
            "socialMediaAccountsList[].URL"
          ]
        }
      ]
    },
    {
      "type": "fieldset",
      "items": [
        {
          "key": "teamMembersList",
          "add": "New Team Member",
          "style": {
            "add": "teamMembersList"
          },
          "items": [
            "teamMembersList[].role",
            "teamMembersList[].name",
            {
              "key": "teamMembersList[].socialMediaAccountsList",
              "add": "New Account",
              "style": {
                "add": "teamMembersList[].socialMediaAccountsList"
              },
              "items": [
                "teamMembersList[].socialMediaAccountsList[].type",
                "teamMembersList[].socialMediaAccountsList[].otherAccountType",
                "teamMembersList[].socialMediaAccountsList[].URL"
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "fieldset",
      "items": [
        {
          "key": "advisorsList",
          "add": "New Advisor",
          "style": {
            "add": "advisorsList"
          },
          "items": [
            "advisorsList[].name"
          ]
        }
      ]
    }
  ]
}
